public class Main {

	public static void main(String[] args)
	{
		for(int i=0; i<2; i++)
		{
			liney();
			for(double j=0; j<3; (j+0.1))
			{
				System.out.println("#                              #");
			}
		}
	}
	public static void liney()
	{
		System.out.println("#------------------------------#");
	}
	public static void spacey()
	{
		System.out.println("#                              #");
	}
}